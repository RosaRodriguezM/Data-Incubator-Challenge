   "source": [
    "### Obtain data\n",
    "ColBeliefs=['id_muj','p15_1_1','p15_1_4']\n",
    "ColRoles=['id_muj']\n",
    "for i in [1,2,5,7]:\n",
    "    for j in range(1,4):\n",
    "        col = 'p17_1_%d_%d'%(i,j)\n",
    "        ColRoles.append(col)\n",
    "Beliefs=pd.read_stata('BD_MUJERES_ENDIREH2016_SitioINEGI.dta',columns=ColBeliefs)\n",
    "Roles=pd.read_stata('BD_MUJERES_ENDIREH2016_SitioINEGI.dta',columns=ColRoles)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "metadata": {
    "collapsed": true
   },
   "outputs": [],
   "source": [
    "### Aggregate data on household roles\n",
    "Roles=Roles.replace(to_replace=['1','4','6','9'],value='F')\n",
    "Roles=Roles.replace(to_replace=['2','5','7','10'],value='M')\n",
    "Roles=Roles.replace(to_replace=['3','8'],value='B')\n",
    "Roles=Roles.replace(to_replace=['11','12','13','14','99'],value='O')\n",
    "Roles=Roles.replace(to_replace='15',value=' ')\n",
    "r=Roles.values[:,1:13]\n",
    "ll=[]\n",
    "for i in range(len(r)):\n",
    "    b=r[i]\n",
    "    b=b[b!=' ']\n",
    "    if len(b)==0:\n",
    "        ll.append(['NA'])\n",
    "    elif any(b=='O'):\n",
    "        ll.append(['O'])\n",
    "    elif any(b=='B'):\n",
    "        ll.append(['B'])\n",
    "    elif any(b=='F') and any(b=='M'):\n",
    "        ll.append(['B'])\n",
    "    elif all(b=='F'):\n",
    "        ll.append(['F'])\n",
    "    elif all(b=='M'):\n",
    "        ll.append(['M'])\n",
    "if len(ll)==len(r):\n",
    "    lll=np.array(ll)\n",
    "# F-all caregiving is done by the interviewee and/or her mother and/or her daugthers and/or her sisters\n",
    "# M-all caregiving is done by the interviewee male partner and/or her father and/or her sons and/or her brothers\n",
    "# B-caregiving is split between male and female members of the interviewee immediate family\n",
    "# O-caregiving is done by someone not in the interviewee immediate family such as a houseworker \n",
    "# NA-there are no caregiving responsabilities in the interviewee's household\n",
    "AggRoles=pd.DataFrame({'id_muj':Roles['id_muj'].values, 'Household roles':np.transpose(lll)[0]})"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "metadata": {
    "collapsed": true
   },
   "outputs": [],
   "source": [
    "# Agreggate data on interviewee's beliefs in gender roles related to caregiving labor\n",
    "b = Beliefs.values[:,1:3]\n",
    "bb = np.zeros(len(b))\n",
    "for i in range(len(b)):\n",
    "    if all(b[i]==['2','1']):\n",
    "        bb[i]='1'\n",
    "    elif all(b[i]==['1','1']):\n",
    "        bb[i]='2'\n",
    "    elif all(b[i]==['1','2']):\n",
    "        bb[i]='3'\n",
    "    elif all(b[i]==['2','2']):\n",
    "        bb[i]='4'\n",
    "#Stament 1: Women should be responsible for the care of the kids, the sick and the elderly\n",
    "#Statement 2: Men should be responsible, like women, for household chores and the care of the kids, the sick and the elderly\n",
    "#1-Disagrees with statement 1, agrees with statement 2 \n",
    "#2-Agrees with both statements\n",
    "#3-Agrees with statement 1, disagrees with statement 2\n",
    "#4-Disagrees with both statements \n",
    "AggBeliefs=pd.DataFrame({'id_muj':Beliefs['id_muj'].values, 'Beliefs':bb})\n",
    "Final=pd.merge(AggRoles,AggBeliefs,on='id_muj')\n",
    "FF=Final.groupby(['Household roles','Beliefs']).size()"
   ]
  },
